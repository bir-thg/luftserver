<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Luftqualität Bewertungen</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
  <div class="container mt-5">
    <h1 class="mb-4">Luftqualität Bewertungen</h1>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>ID</th>
          <th>Bewertung</th>
          <th>Breitengrad</th>
          <th>Längengrad</th>
          <th>Land</th>
          <th>Ort</th>
          <th>Strasse</th>
          <th>PLZ</th>
          <th>Zeitstempel</th>
        </tr>
      </thead>
      <tbody>
        <% ratings.forEach(function(rating) { %>
          <tr>
            <td><%= rating.id %></td>
            <td><%= rating.rating %></td>
            <td><%= rating.latitude %></td>
            <td><%= rating.longitude %></td>
            <td><%= rating.country %></td>
            <td><%= rating.locality %></td>
            <td><%= rating.street %></td>
            <td><%= rating.postalCode %></td>
            <td><%= rating.timestamp %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</body>
</html>
